{
    "title": "Caps and return to ctrl. Ctrl to caps.",
    "rules": [{
        "description": "Command+Tab <-> Alt+Tab",
        "manipulators": [{
            "type": "basic",
            "from": {
                "key_code": "tab",
                "modifiers": {
                    "mandatory": [
                        "left_command"
                    ]
                }
            },
            "to": [{
                "key_code": "tab",
                "modifiers": [
                    "left_option"
                ]
            }]
        }, {
            "type": "basic",
            "from": {
                "key_code": "tab",
                "modifiers": {
                    "mandatory": [
                        "left_option"
                    ]
                }
            },
            "to": [{
                "key_code": "tab",
                "modifiers": [
                    "left_command"
                ]
            }]
        }]
    }, {
        "description": "Ctrl+n/p <-> up/down",
        "manipulators": [{
            "type": "basic",
            "from": {
                "key_code": "p",
                "modifiers": {
                    "mandatory": [
                        "left_control"
                    ]
                }
            },
            "to": [{
                "key_code": "up_arrow"
            }]
        }, {
            "type": "basic",
            "from": {
                "key_code": "n",
                "modifiers": {
                    "mandatory": [
                        "left_control"
                    ]
                }
            },
            "to": [{
                "key_code": "down_arrow"
            }]
        }]
    }]
}
