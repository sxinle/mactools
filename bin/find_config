#!/bin/bash

findName=*$1*
#echo $findName

find ~/Library/Application\ Support/ -iname $findName
find ~/Library/Preferences/ -iname $findName
find ~/Library/Caches/ -iname $findName


sudo find /Library/Application\ Support/ -iname $findName
sudo find /Library/Preferences/ -iname $findName
find /Library/Caches/ -iname $findName

